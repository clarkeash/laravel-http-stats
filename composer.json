{
    "name": "clarkeash/laravel-http-stats",
    "description": "Get access to stats for your http requests",
    "type": "library",
    "require": {
        "php": "^8.2",
        "laravel/framework": "^11.0",
        "guzzlehttp/guzzle": "^7.8"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.0",
        "orchestra/testbench": "^9.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Ashley Clarke",
            "email": "me@ashleyclarke.me"
        }
    ],
    "autoload": {
        "psr-4": {
            "Clarkeash\\LaravelHttpStats\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Test\\Clarkeash\\LaravelHttpStats\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Clarkeash\\LaravelHttpStats\\Providers\\ServiceProvider"
            ]
        }
    },
    "config": {
        "preferred-install": "dist"
    }
}
